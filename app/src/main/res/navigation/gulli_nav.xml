<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gulli_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.gulliver_v2.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_homeFragment_to_cartFragment"
            app:destination="@id/cartFragment" />
        <action
            android:id="@+id/action_homeFragment_to_GMapsFragment"
            app:destination="@id/GMapsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.gulliver_v2.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_mapFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.gulliver_v2.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart">
        <action
            android:id="@+id/action_cartFragment_to_mainActivity4"
            app:destination="@id/mainActivity4" />
        <action
            android:id="@+id/action_cartFragment_to_overviewFragment"
            app:destination="@id/overviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/GMapsFragment"
        android:name="com.example.gulliver_v2.GMapsFragment"
        android:label="fragment_g_maps"
        tools:layout="@layout/fragment_g_maps" >
        <action
            android:id="@+id/action_GMapsFragment_to_mainActivity4"
            app:destination="@id/mainActivity4" />
        <action
            android:id="@+id/action_GMapsFragment_to_itineraryFragment"
            app:destination="@id/itineraryFragment" />
    </fragment>
    <activity
        android:id="@+id/mainActivity4"
        android:name="com.example.gulliver_v2.MainActivity"
        android:label="MainActivity" />
    <fragment
        android:id="@+id/overviewFragment"
        android:name="com.example.gulliver_v2.OverviewFragment"
        android:label="fragment_overview"
        tools:layout="@layout/fragment_overview" >
        <action
            android:id="@+id/action_overviewFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:popUpTo="@id/cartFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_overviewFragment_to_itineraryFragment"
            app:destination="@id/itineraryFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.gulliver_v2.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" >
        <action
            android:id="@+id/action_chatFragment_to_mainActivity4"
            app:destination="@id/mainActivity4"
            app:popUpTo="@id/mainActivity4"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/itineraryFragment"
        android:name="com.example.gulliver_v2.ItineraryFragment"
        android:label="fragment_itinerary"
        tools:layout="@layout/fragment_itinerary" >
        <action
            android:id="@+id/action_itineraryFragment_to_GMapsFragment"
            app:destination="@id/GMapsFragment"
            app:popUpTo="@id/GMapsFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_itineraryFragment_to_overviewFragment"
            app:destination="@id/overviewFragment" />
    </fragment>
</navigation>